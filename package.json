		"jsdom": "^11.10.0",
		"less": "^3.0.4",
		"mocha": "^5.2.0",
		"react-addons-test-utils": "^15.6.2",
		"webpack": "^4.8.3",
		"webpack-cli": "^2.1.3",
		"webpack-dev-server": "^3.1.4"
	},
	"dependencies": {
		"lodash": "^4.17.10",
